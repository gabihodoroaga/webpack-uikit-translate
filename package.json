{
  "name": "webpack-uikit-translate",
  "version": "1.0.0",
  "description": "A starter project for creating multi-language static website using Uikit and Webpack",
  "main": "index.js",
  "scripts": {
    "dev": "webpack --mode development",
    "start": "webpack serve",
    "build": "export NODE_ENV=production && webpack --mode production",
    "tr-export": "node translate-export",
    "dev-es": "webpack --mode development --env locale=es",
    "start-es": "webpack serve --env locale=es",
    "build-es": "export NODE_ENV=production && webpack --mode production --env locale=es",
    "build-all": "npm run build && npm run build-es"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gabihodoroaga/webpack-uikit-translate.git"
  },
  "keywords": [
    "webpack",
    "uikit",
    "translate"
  ],
  "author": "Gariel Hodoroaga",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/gabihodoroaga/webpack-uikit-translate/issues"
  },
  "homepage": "https://github.com/gabihodoroaga/webpack-uikit-translate#readme",
  "devDependencies": {
    "@types/lodash": "^4.14.165",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.3.2",
    "css-loader": "^5.0.1",
    "file-loader": "^6.2.0",
    "globs": "^0.1.4",
    "gulp-translate": "^1.5.9",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^4.5.0",
    "mini-css-extract-plugin": "^1.3.1",
    "sass": "^1.29.0",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "translation-loader": "^1.1.2",
    "ts-loader": "^8.0.11",
    "typescript": "^4.1.2",
    "uikit": "^3.5.9",
    "url-loader": "^4.1.1",
    "webpack": "^5.6.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  }
}
